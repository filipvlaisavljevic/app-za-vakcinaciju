        <activity android:name=".brojLjudiZaVakcinaciju" />
        <activity android:name=".SazetakPrethodnihOdgovora" />
        <activity android:name=".PrioritetnaGrupa" />
        <activity android:name=".ProvjeraKorisnika" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>