<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vakcinacija_stefanfilip">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Prijava za vakcinaciju"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VakcinacijaStefanFilip">
        <activity android:name=".PrijavaActivity"></activity>
        <activity android:name=".StatistikaActivity" />
        <activity android:name=".BrojActivity" />
        <activity android:name=".SazetakActivity" />
        <activity android:name=".PrioritetnaActivity" />
        <activity android:name=".BolestActivity" />
        <activity android:name=".PocetniActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>